{
  "name": "desxa-companion",
  "displayName": "Desxa - Mascota Virtual",
  "description": "Un delf√≠n virtual llamado Desxa",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "desxa-companion.showDesxa",
        "title": "Desxa: Ver estado"
      },
      {
        "command": "desxa-companion.playWithDesxa",
        "title": "Desxa: Jugar"
      },
      {
        "command": "desxa-companion.alimentarDesxa",
        "title": "Desxa: Alimentar"
      },
      {
        "command": "desxa-companion.acariciarDesxa",
        "title": "Desxa: Acariciar"
      },
      {
        "command": "desxa-companion.descansarDesxa",
        "title": "Desxa: Descansar"
      },
      {
        "command": "desxa-companion.showPanel",
        "title": "Desxa: Mostrar mascota en panel"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "16.x"
  }
}
